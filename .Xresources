#include </home/yuune/.xrdb/vionoir>

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

URxvt.intensityStyles:  false
URxvt.font:             xft:Hack:bold:size=8
URxvt.boldFont:         xft:Hack:bold:size=8
URxvt.saveLines:        8192

URxvt*scrollTtyOutput:    false
URxvt*scrollWithBuffer:   true
URxvt*scrollTtyKeypress:  true
URxvt.scrollBar:          false
URxvt.internalBorder:     26
URxvt.cursorBlink:        false
URxvt.cursorUnderline:    true

rofi.font:             Noto Sans Bold 8
rofi.modi:             run
rofi.width:            20
rofi.lines:            4
rofi.columns:          1
rofi.bw:               5
rofi.padding:          0
rofi.line-margin:      0
rofi.line-padding:     5
rofi.location:         7
rofi.xoffset:          100
rofi.yoffset:          -100
rofi.separator-style:  none
rofi.hide-scrollbar:   true
rofi.sidebar-mode:     false
